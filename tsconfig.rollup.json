{
  "compilerOptions": {
    "module": "es6",
    "target": "es2021",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true
  }
}
